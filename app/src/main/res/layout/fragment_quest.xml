<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ProfileActivity$QuestFragment">

   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="vertical">


       <!--Using Material to implement the card view-->
       <com.google.android.material.card.MaterialCardView
           android:id="@+id/card"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:clickable="true"
           android:layout_margin="8dp">

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="horizontal">

               <LinearLayout
                   android:layout_width="148dp"
                   android:layout_height="wrap_content"
                   android:layout_weight="1"
                   android:orientation="vertical">

                   <ImageView
                       android:layout_width="100dp"
                       android:layout_height="100dp"
                       android:contentDescription="@string/image"
                       android:scaleType="centerCrop"
                       android:src="@drawable/black_girl_prof"></ImageView>
               </LinearLayout>


               <LinearLayout
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:orientation="vertical"
                   android:layout_weight="3.5"
                   android:paddingTop="8dp"
                   android:gravity="center">

                   <!--Card Quest Title, secondary text-->
                   <TextView
                       android:id="@+id/fragQuestTitle"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:text="Going to the Gym"
                       android:textSize="15dp"
                       android:textAppearance="?attr/textAppearanceHeadline6" />

                   <TextView
                       android:id="@+id/fragQuestDescription"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="8dp"
                       android:text="Come by, lets get it!!"
                       android:textAppearance="?attr/textAppearanceBody2"
                       android:textColor="?android:attr/textColorSecondary" />

                   <TextView
                       android:id="@+id/fragQuestDate"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="8dp"
                       android:text="10/12/23"
                       android:textAppearance="?attr/textAppearanceBody2"
                       android:textColor="?android:attr/textColorSecondary" />
               </LinearLayout>

               <LinearLayout
                   android:layout_width="148dp"
                   android:layout_height="wrap_content"
                   android:layout_weight="1"
                   android:orientation="vertical">

                   <ImageView
                       android:layout_width="30dp"
                       android:layout_height="100dp"
                       android:layout_marginLeft="30dp"
                       android:contentDescription="@string/image"
                       android:src="@drawable/baseline_access_time_24"></ImageView>
               </LinearLayout>
           </LinearLayout>

           <!--Action Buttons-->
           <!--   <LinearLayout
                  android:layout_width="wrap_content"
